<?xml version="1.0" encoding="UTF-8"?>
<plugin
   id="com.cougaarsoftware.cougaar.ide.launcher.core"
   name="%Plugin.name"
   version="1.0.3"
   provider-name="%Provider.name"
   class="com.cougaarsoftware.cougaar.ide.launcher.core.LauncherPlugin">

   <runtime>
      <library name="cougaar-ide-launcher-core.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.jdt.launching"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.swt"/>
      <import plugin="org.eclipse.jdt.core"/>
   </requires>


   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            name="%com.cougaarsoftware.cougaar.ide.launcher.core.configuration.CougaarLaunchConfigurationType"
            delegate="com.cougaarsoftware.cougaar.ide.launcher.core.configuration.CougaarLaunchConfigurationDelegate"
            modes="run, debug"
            id="com.cougaarsoftware.cougaar.ide.launcher.core.configuration.CougaarLaunchConfigurationType">
      </launchConfigurationType>
   </extension>

</plugin>
